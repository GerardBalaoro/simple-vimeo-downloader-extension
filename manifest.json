{
  "manifest_version": 2,
  "name": "Simple Vimeo Downloader",
  "version": "1.0.4",
  "description": "Simple Vimeo Downloader extension allows you to download video from Vimeo by injecting download buttons directly into Vimeo player.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_title": "Inject Simple Vimeo Downloader buttons",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "*://vimeo.com/*",
    "*://player.vimeo.com/*"
  ],
  "web_accessible_resources": [
    "inject_icon.svg"
  ]
}
